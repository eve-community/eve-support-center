<UserControl
	x:Class="EVE_Support_Center.Pages.MyV.MyVView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:EVE_Support_Center.Pages.MyV"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="300"
	d:DesignWidth="400"
	mc:Ignorable="d">

	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="../../Resources/GeneralStyles.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</UserControl.Resources>

	<StackPanel Orientation="Vertical">
		<TextBlock Style="{StaticResource GroupHeaderStyle}" Text="my V" />
		<TextBlock Style="{StaticResource GroupSubHeaderStyle}" Text="{Binding DeviceInfo}" />
	</StackPanel>
</UserControl>
